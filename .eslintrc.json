{
  "env": {
    "browser": true,
    "es2021": true
  },
  "plugins": ["@html-eslint"],
  "extends": ["eslint:recommended"],
  "parserOptions": {
    "sourceType": "module"
  },
  "overrides": [
    {
      "files": ["*.html"],
      "parser": "@html-eslint/parser",
      "extends": ["plugin:@html-eslint/recommended"]
    },
    {
      "files": ["*.js"],
      "extends": ["plugin:prettier/recommended"]
    }
  ],
  "rules": {}
}
